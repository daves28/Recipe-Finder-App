{
  "name": "recipe-finder",
  "version": "1.0.0",
  "description": "Fetch and display recipes from an API based on user input ingredients",
  "main": "src/index.js",
  "scripts": {
    "start": "set \"NODE_ENV=production\" && nodemon src/server/server.js",
    "build": "set \"NODE_ENV=production\" && \"webpack\"",
    "dev": "set \"NODE_ENV=development\" && concurrently \"webpack-dev-server --open\" \"nodemon src/server/server.js\""
  },
  "nodemonConfig": {
    "ignore": [
      "build",
      "client"
    ]
  },
  "author": "David Soerensen",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-syntax-class-properties": "^7.8.3",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "mini-css-extract-plugin": "^2.7.2",
    "path": "^0.12.7",
    "pg": "^8.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-image-fade-in": "^1.1.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7.3",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.2.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3"
  }
